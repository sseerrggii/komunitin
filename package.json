{
  "name": "komunitin",
  "version": "0.0.1",
  "description": "Open System for Group Communities",
  "productName": "Komunitin",
  "cordovaId": "org.komunitin.app",
  "capacitorId": "",
  "author": "komunitin <komunitinbox@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m pwa --env.USE_MIRAGE 200",
    "build": "quasar build -m pwa --env.USE_MIRAGE 200",
    "start": "quasar serve --history dist/pwa",
    "test": "quasar test --unit jest",
    "test:coverage": "jest --silent --coverage src/",
    "test:watch": "jest --watch src/",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --max-warnings 0 ./src",
    "doc": "typedoc --out docs --tsconfig tsconfig.json src",
    "todo": "egrep -r '@todo|@bug|@dev' src/",
    "testAll": "tsc -p tsconfig.json --noEmit && npm run test && npm run lint"
  },
  "dependencies": {
    "@alexlit/vue-yandex-share": "^1.0.1",
    "@electron/typescript-definitions": "^8.7.3",
    "@quasar/extras": "^1.6.4",
    "@types/clamp-js": "^0.7.2",
    "@types/dotenv": "^8.2.0",
    "@types/leaflet": "^1.5.12",
    "@types/marked": "^0.7.3",
    "@types/vuelidate": "^0.7.12",
    "axios": "^0.19.2",
    "clamp-js": "^0.7.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "leaflet": "^1.6.0",
    "markdown-to-txt": "^1.0.1",
    "marked": "^0.8.2",
    "node-sass": "^4.13.1",
    "quasar": "^1.9.14",
    "typedoc": "^0.17.4",
    "vue-element-loading": "^1.1.5",
    "vue-i18n": "^8.16.0",
    "vue2-leaflet": "^2.5.2",
    "vuelidate": "^0.7.5"
  },
  "devDependencies": {
    "@quasar/app": "^1.6.2",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.0",
    "@quasar/quasar-app-extension-typescript": "^1.0.0-beta.2",
    "@types/node": "^12.12.35",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "acorn": ">=6.4.1",
    "devtron": "^1.4.0",
    "electron": "^8.2.1",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^14.2.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^25.3.0",
    "kind-of": ">=6.0.3",
    "lorem-ipsum": "^2.0.3",
    "minimist": ">=1.2.3",
    "miragejs": "^0.1.37",
    "ts-jest": "^25.3.1",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "vue-jest": "^3.0.5"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
