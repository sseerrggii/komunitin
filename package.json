    "@typescript-eslint/parser": "^1.12.0",
    "devtron": "^1.4.0",
    "electron": "^7.1.12",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-vue": "^6.2.1",
    "lorem-ipsum": "^2.0.3",
    "miragejs": "^0.1.35",
    "ts-jest": "^25.2.1",
    "typescript": "^3.3.3"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
