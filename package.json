{
  "name": "komunitin",
  "version": "0.0.1",
  "description": "Open System for Exchange Communities",
  "productName": "Komunitin",
  "cordovaId": "org.komunitin.app",
  "capacitorId": "",
  "author": "komunitin <komunitinbox@gmail.com>",
  "private": true,
  "scripts": {
    "test": "quasar test --unit jest",
    "test:coverage": "jest --coverage src/",
    "test:watch": "jest --watch src/",
    "dev": "quasar dev",
    "dev:mirage": "quasar dev --env.USE_MIRAGE 1000",
    "build": "quasar build",
    "build:pwa": "quasar build -m pwa",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore ./src",
    "doc": "typedoc --out docs --tsconfig tsconfig.json src",
    "todo": "egrep -r '@todo|@bug|@dev' src/"
  },
  "dependencies": {
    "@alexlit/vue-yandex-share": "^1.0.1",
    "@quasar/extras": "^1.5.1",
    "@types/dotenv": "^8.2.0",
    "@types/leaflet": "^1.5.9",
    "@types/vuelidate": "^0.7.10",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "leaflet": "^1.6.0",
    "quasar": "^1.9.3",
    "typedoc": "^0.16.10",
    "vue-element-loading": "^1.1.5",
    "vue-i18n": "^8.0.0",
    "vue2-leaflet": "^2.5.2",
    "vuelidate": "^0.7.5"
  },
  "devDependencies": {
    "@quasar/app": "^1.5.8",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
